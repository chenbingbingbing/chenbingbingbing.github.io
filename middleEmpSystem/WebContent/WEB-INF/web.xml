<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>middleEmpSystem</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>EmpServlet</display-name>
    <servlet-name>EmpServlet</servlet-name>
    <servlet-class>com.baidu.controller.EmpServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>EmpServlet</servlet-name>
    <url-pattern>/empServlet</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>PageServlet</display-name>
    <servlet-name>PageServlet</servlet-name>
    <servlet-class>com.baidu.controller.PageServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>PageServlet</servlet-name>
    <url-pattern>/PageServlet</url-pattern>
  </servlet-mapping>
  <listener>
    <listener-class>com.baidu.filter.ServletContextTest</listener-class>
  </listener>
  <listener>
    <listener-class>com.baidu.filter.ServletSessionTest</listener-class>
  </listener>
  <listener>
    <listener-class>com.baidu.filter.OnlineTest</listener-class>
  </listener>
  <servlet>
    <description></description>
    <display-name>OnlineServlet</display-name>
    <servlet-name>OnlineServlet</servlet-name>
    <servlet-class>com.baidu.controller.OnlineServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>OnlineServlet</servlet-name>
    <url-pattern>/onlineServlet</url-pattern>
  </servlet-mapping>
</web-app>